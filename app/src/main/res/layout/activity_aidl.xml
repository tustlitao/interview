<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".aidl.AIDLActivity">


    <TextView
            android:text="现在啥还没有"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tvPersonInfo" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintVertical_bias="0.256" android:textSize="24sp"/>
    <Button
            android:text="BindService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnBindService" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="56dp" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/tvPersonInfo"/>
    <Button
            android:text="UnBinderService"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnUnBindService"
            android:layout_marginTop="48dp" app:layout_constraintTop_toBottomOf="@+id/tvPersonInfo"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="56dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>